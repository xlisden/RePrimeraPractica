CREATE DEFINER=`root`@`localhost` PROCEDURE `spUpdateCliente`(
	IN idcliente INT,
	IN nombres VARCHAR(45),
    IN apellidos VARCHAR(45),
    IN dni VARCHAR(8),
    IN fechaNaciemiento VARCHAR(45),
    IN direccion VARCHAR(100)
)
BEGIN
	UPDATE `primerapracticapooii`.`cliente`
	SET
	`nombres` = nombres,
	`apellidos` = apellidos,
	`dni` = dni,
	`fechaNacimiento` = STR_TO_DATE(fechaNacimiento, '%Y-%m-%d'),
	`direccion` = direccion
	WHERE `cliente`.`idcliente` = idcliente;
END ;;
DELIMITER ;
