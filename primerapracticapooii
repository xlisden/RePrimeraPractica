CREATE DEFINER=`root`@`localhost` PROCEDURE `spReadPrestamosCliente`(
	IN idcliente INT
)
BEGIN
	SELECT p.idprestamo, p.fechaPrestamo, p.monto, CONCAT(c.nombres, ' ', c.apellidos) as cliente, p.interes, p.nroCuotas
    FROM prestamo p
    INNER JOIN cliente c ON c.idcliente = p.idcliente
    WHERE p.idcliente = idcliente;
END ;;
DELIMITER ;
