CREATE DEFINER=`root`@`localhost` PROCEDURE `spUpdatePrestamo`(
	IN idprestamo INT,
	IN fechaPrestamo VARCHAR(45),
    IN monto INT,
    IN idcliente INT,
    IN interes INT,
    IN nroCuotas INT
)
BEGIN

	UPDATE `primerapracticapooii`.`prestamo`
	SET
	`fechaPrestamo` = STR_TO_DATE(fechaPrestamo, '%Y-%m-%d'),
	`monto` = monto,
	`idcliente` = idcliente,
	`interes` = interes,
	`nroCuotas` = nroCuotas
	WHERE `prestamo`.`idprestamo` = idprestamo;

END ;;
